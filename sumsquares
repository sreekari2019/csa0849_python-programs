l = []
n=int(input("enter range of list: "))
for i in range(n):
    elements= int(input("enter list elements: "))
    l.append(elements)
print(l)
def sumsquares(l):
    odd=0
    even=0
    for i in l:
        if i%2==0:
            even=even+i**2
        else:
            odd=odd+i**2
    l=[odd,even]
    return l
print(sumsquares(l))  
